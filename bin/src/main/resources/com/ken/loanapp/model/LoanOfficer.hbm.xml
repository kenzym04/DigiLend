<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="loan_db" name="com.ken.loanapp.model.LoanOfficer" optimistic-lock="version" table="loan_officer">
    <id name="officerIdNo" type="int">
      <column name="officer_id_no"/>
      <generator class="foreign">
        <param name="property">user</param>
      </generator>
    </id>
    <one-to-one class="com.ken.loanapp.model.User" constrained="true" name="user"/>
    <property name="officerName" type="string">
      <column length="30" name="officer_name"/>
    </property>
  </class>
</hibernate-mapping>
